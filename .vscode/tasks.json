{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "xcodebuild: Build Going Zero (Debug)",
      "type": "shell",
      "command": "xcodebuild",
      "args": [
        "-project",
        "*.xcodeproj",
        "-scheme",
        "Going Zero",
        "-configuration",
        "Debug"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": []
    }
  ]
}